		"husky": "4.2.3",
		"jest": "25.1.0",
		"mocha": "7.1.1",
		"npm-run-all": "4.1.5",
		"nyc": "15.0.0",
		"semantic-release": "17.0.4",
		"vscode-test": "1.3.0"
	},
